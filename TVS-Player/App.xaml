<Application x:Class="TVS_Player.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TVS_Player"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary x:Name="Dictionaries">
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/TVS-Player;component/Styles/Brushes.xaml"/>
                <ResourceDictionary Source="/TVS-Player;component/Styles/Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
